//Sum All Numbers in a Range
function sumAll(arr) {
  let small = arr[0]<arr[1] ? arr[0] : arr[1]
  let large = arr[0]<arr[1] ? arr[1] : arr[0]
  let arrNew = []
  for (i=small;i<=large;i++){
    arrNew.push(i)
  }
  let sum = arrNew.reduce((acc,curr)=>{
    return acc+curr
  })
  return sum;
}

console.log(sumAll([1, 4]));
